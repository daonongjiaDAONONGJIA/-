page{
  padding-bottom: 140rpx;
}
.wrap{
  background: #fff;
}
.date-show{ 
  margin-top:30rpx;
  font-family: PingFang-SC-Regular; 
  font-size: 40rpx; 
  color: #282828; 
  text-align: center; 
  display: flex;
  justify-content: space-between;
} 
.date-show .date{
  margin:0 30rpx;
}
.lt-arrow image,.rt-arrow image{ 
  width: 58rpx; 
  height: 58rpx; 
} 
.header{ 
  font-size: 0; 
  padding: 0 24rpx; 
} 
.header>view{ 
  display: inline-block; 
  width: 14.285%; 
  color: #333; 
  font-size: 30rpx; 
  text-align: center; 
  border-bottom: 1px solid #D0D0D0; 
  padding: 30rpx 0; 
} 
.weekMark{ 
  position: relative; 
} 
.weekMark view{ 
  position: absolute; 
  bottom: 0; 
  left: 0; 
  width: 100%; 
  border-bottom: 1px solid #f93a3c; 
} 
.date-box{ 
  font-size: 0; 
  padding: 10rpx 0; 
} 
.date-box>view{ 
  position: relative; 
  display: inline-block; 
  width: 14.285%; 
  color: #020202; 
  font-size: 40rpx; 
  text-align: center; 
  vertical-align: middle; 
  margin: 15rpx 0; 
} 
.date-head{ 
  height: 60rpx; 
  line-height: 60rpx; 
  font-size: 26rpx; 
} 
.selectDay .date-head{
  color:#f93a3c
} 
.selectedDay .date-head{ 
  width: 60rpx; 
  border-radius: 50%; 
  text-align: center; 
  color: #fff; 
  background-color: #f93a3c; 
  margin: 0 auto; 
}
.date-weight{ 
  font-size: 22rpx; 
  padding: 8rpx 0; 
} 
.selectDay .date-weight,.selectedDay .date-weight{ 
  color: #f93a3c; 
} 
.one{ 
  position: absolute; 
  bottom: 0; 
  right: 5rpx; 
  width: 20rpx; 
  height: 20rpx; 
  border-radius: 50%; 
  background-color: red; 
} 
.two{ 
  position: absolute; 
  bottom: 30rpx; 
  right: 5rpx; 
  width: 20rpx; 
  height: 20rpx; 
  border-radius: 50%; 
  background-color: blue; 
} 

.order-section{
  margin-top:20rpx;
  padding: 0 30rpx 30rpx 30rpx;
  background: #fff;
}
.order-section .s-tit{
  display: flex;
  justify-content: space-between;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 34rpx;
}
.order-section .s-con{
  font-size: 28rpx;
}
.insure{
  display: flex;
  justify-content: space-between;
}
.sel-xm image{
  width: 40rpx;
  height: 40rpx;
}
.house-list{
  margin-bottom: 10rpx;
  font-size: 28rpx;
  display: flex;
  align-items: center;
}
.house-list image{
  width: 50rpx;
  height: 50rpx;
  margin-right: 16rpx;
}
.house-list input{
  width: 80rpx;
  height: 24rpx;
  line-height: 24rpx;
  border-bottom: 1px solid #000;
  text-align: center;
}
.use-money{
  display: flex;
  align-items: center;
}
.house-item{
  padding: 10rpx;
  box-sizing: border-box;
  border:1px solid #dedede;
  flex-shrink: 0;
  width:30%;
  margin-right: 5%;
  font-size: 28rpx;
  text-align: center
}
.house-item:nth-of-type(3n){
  margin-right:0;
}
.house-item.active{
  border:1px solid #f65c5e;
  background:#f65c5e;
  color: #fff;
}
.yh-info{
  display: flex;
  justify-content: space-between;
}
.sel-yh{
  display: flex;
  align-items: center;
}
.change-num{
  display: flex;
  align-items: center;
}
.change-num .change-btn{
  width: 60rpx;
  height: 60rpx;
  text-align: center;
  line-height: 60rpx;
  border:1px solid #f65c5e;
  color: #f65c5e;
  border-radius: 50%;
}
.change-num .change-btn.none{
  border:1px solid #ccc;
  color: #ccc;
}
.change-num .change-num1{
   width: 60rpx;
   height: 60rpx;
   text-align: center;
   line-height: 60rpx;
}
.change-num .change-btn{
  font-size:40rpx;
}
.user-info{
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 30rpx 0;
  border:1px solid #f7f7f7;
  background: #fff;
}
.user-info .info{
  display: flex;
  font-size: 28rpx;
}
.user-info .user-tit{
  width: 200rpx;
}
.changeContact{
  width: 80rpx;
  height: 80rpx;
  overflow: hidden;
}
.changeContact image{
  width: 60rpx;
  height: 60rpx;
}
.contact-list .contact-item{
  display: flex;
  justify-content: space-between;
  align-content: center;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 28rpx;
}
.contact-list .contact-item .contact-user{
  display: flex;
}
.contact-list .contact-item .contact-user .username{
  margin-right: 10px;
}
.footer{
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 20rpx 30rpx;
  position: fixed;
  bottom:0;
  left: 0;
  width: 100%;
  background: #fff;
  border-top:1px solid #dedede;
  box-sizing: border-box
}
.modals{position:fixed; z-index: 999; top:0; left: 0; right:0; bottom: 0;}

.modals-cancel{position:absolute; z-index:1000; top:0; left: 0; right:0; bottom: 0; background-color: rgba(0,0,0,.5);}

.bottom-dialog-body{
  position:absolute; 
  z-index:10001; 
  bottom:0; 
  left:0; 
  right:0; 
  padding:30rpx; 
  background-color: #fff;
}
.bottom-pos{-webkit-transform:translateY(100%);transform:translateY(100%);}
.voucher-list{
  max-height: 600rpx;
  padding-top: 30rpx;
  overflow-y: scroll
}
.voucher-item{
  display: flex;
  margin-bottom: 30rpx;
  align-items: center;
}
.voucher-item .voucher-name{
  margin-bottom: 10rpx;
  font-size:30rpx;
}
.voucher-item .voucher-use{
  padding: 8rpx 0;
  color: #666;
  font-size: 28rpx;
}
.voucher-item .voucher-price{
  position: relative;
  flex-shrink: 0;
  width: 40%;
  text-align: center;
  font-size:38rpx;
  margin-right: 30rpx;
}
.voucher-item .voucher-price image{
  width: 100%;
  height: 100%
}
.voucher-item .voucher-price .price{
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #fff;
}
.voucher-none{
  padding: 30rpx;
  text-align: center;
  font-size: 30rpx;
}
.voucher-info{
  width: 100%;
}
.voucher-sel{
  flex-shrink: 0;
  text-align: center;
   width: 60rpx;
}
.voucher-sel image{
  width: 50rpx;
  height: 50rpx;
}

.fare-con{
  margin-bottom: 20rpx;
  /* display: flex;
  align-items: center */
}
.fare-con .tit{
  height: 80rpx;
  line-height: 80rpx;
}
.car-type{
  display: flex;
}
.car-type .type-item{
  padding-bottom: 10rpx;
  position: relative;
  margin-right: 30rpx;
}
.car-type .type-item:last-of-type{
  margin-right: 0
}
.car-type .type-item.active::after{
  position: absolute;
  content: "";
  left: 50%;
  bottom:0;
  width:50%;
  margin-left: -25%;
  height:2px;
  background: #f65c5e;
}
.js-type{
  display: flex;
}
.js-type .type-item{
  padding: 6rpx 20rpx;
  border-radius: 30px;
  margin-right: 30rpx;
}
.js-type .type-item:last-of-type{
  margin-right: 0
}
.js-type .type-item.active{
  background: #f65c5e;
  color: #fff;
}
.address-list{
  display: flex;
}
.address-list .address-item{
  padding: 6rpx 20rpx;
  border-radius: 6px;
  margin-right: 30rpx;
  background:#eee;
  color: #666; 
}
.address-list .address-item:last-of-type{
  margin-right: 0
}
.address-list .address-item.active{
  background: #fac5c6;
  color: #f65c5e;
}
.hidden{
  display: none
}
.car-money{
  color: #f65c5e;
  font-size: 30rpx;
}