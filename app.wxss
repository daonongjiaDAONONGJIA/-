.goods-none{
  height: 300rpx;
  line-height: 300rpx;
  text-align: center;
  font-size: 30rpx;
  color: #666
}


.goods-discount{
  padding:0 30rpx;
  background: #fff;
  margin-top:20rpx;
  font-size: 26rpx;
}
.goods-discount .discount-item{
  padding:30rpx 0;
  width: 100%;
  display: flex;
  justify-content: space-between;
  border-bottom: 1px solid #ededed;
}
.goods-discount .discount-item:last-of-type{
  border:none;
}
.goods-discount .discount-item .tit{
  display: flex;
  align-items: center;
}
.goods-discount .discount-item .tit image{
  width: 44rpx;
  height: 44rpx;
  margin-right: 20rpx;
}
.goods-discount .discount-item .link{
  display: flex;
  align-items: center;
  color: #838383;
}
.goods-discount .discount-item .link image{
  width: 40rpx;
  height: 40rpx;
}

.shop-style,.shop-map{
  padding: 0 30rpx 30rpx 30rpx;
  margin-top: 20rpx;
  background: #fff;
}
.shop-style .tit,.shop-map .tit{
  height: 90rpx;
  line-height: 90rpx;
  font-size: 32rpx;
}
.shop-style .style{
  display: flex;
  align-items: center;
  justify-content: space-between;
  width: 100%;
}
.shop-style .shop-info{
  display: flex;
  align-items: center;
  font-size: 28rpx;
}
.shop-style .shop-logo{
  margin-right: 26rpx;
  flex-shrink: 0;
  width: 90rpx;
  height: 90rpx;
  background: #eee;
  border-radius: 50%;
  overflow: hidden
}
.shop-style .shop-logo image{
   width: 90rpx;
   height: 90rpx;
}
.shop-style .shop-tag{
  margin-top: 10rpx;
  font-size: 24rpx;
  color: #666
}
.shop-style .style-btn{
  flex-shrink: 0;
  padding: 0 30rpx;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  background: #07c764;
  color: #fff;
  border-radius: 30rpx;
  font-size: 24rpx;
}
.shop-map{
  position: relative
}
.shop-map .map{
  height: 350rpx;
}
.shop-map .map-con{
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 30rpx;
  position: absolute;
  left: 10%;
  bottom:50rpx;
  width:80%;
  background: #fff;
  border-radius: 10rpx;
  font-size: 28rpx;
  box-sizing: border-box;
}
.shop-map .map-con .info{

}
.shop-map .map-con .info .name{
  font-size: 30rpx;
  margin-bottom: 10rpx;
}
.shop-map .map-con .info .desc{
  font-size: 24rpx;
  color: #838383;
}
.shop-map .map-con .m-btn{
   margin-left: 30rpx;
   flex-shrink: 0;
   color: #07c764;
   font-size: 24rpx;
}


.discount{
  padding:0 30rpx;
  margin-top: 20rpx;
  margin-bottom: 100rpx;
  background: #fff;
}
.discount .discount-item1{
  margin-bottom: 28rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 28rpx;
}
.discount .discount-item1:first-of-type{
  padding-top: 30rpx;
}
.discount .discount-item1:last-of-type{
  margin-bottom: 0;
  padding-bottom: 30rpx;
}
.discount .discount-item1 .tit{
  display: flex;
  align-items: center;
  justify-content: center
}
.discount .discount-item1 .con{
  display: flex;
  align-items: center;
  margin-left: 20rpx;
  flex-shrink: 0;
}
.discount .discount-item1 image{
  margin-left: 30rpx;
  width: 40rpx;
  height: 40rpx;
}

.mask{
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background: rgba(0,0,0,0.4);
  display: flex;
  align-items: center;
  justify-content: center;
}
.auth{
  padding: 30rpx;
  width: 80%;
  box-sizing: border-box;
  background: #fff;
  border-radius: 10rpx;
}
.auth .auth-btn{
  margin:0 10%;
  width: 80%;
  height: 80rpx;
  line-height: 80rpx;
  background: #feb01a;
  color: #fff;
  font-size: 24rpx;
}