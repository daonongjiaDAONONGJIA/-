/*首页*/

.c-title {
  font-size: 16px;
  font-weight: bold;
  margin-top: 15px;
}

.cate {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-around;
  padding-top: 15px;
  margin-left: -15px;
  margin-right: -15px;
}

.item {
  width: 20%;
  margin-bottom: 25px;
}

.cate-img image {
  width: 65rpx;
  height: 65rpx;
}

.cate-img {
  text-align: center;
}

.cate-title {
  font-size: 12px;
  text-align: center;
  color: #777;
}

/*通用列表*/

.list {
  display: flex;
  justify-content: space-around;
}

.list-img {
  width: 33%;
  margin-right: 15px;
}

.list-img image {
  max-width: 100%;
}

.list-text {
  flex: 1;
}

.list-title {
  font-weight: bold;
  font-size: 15px;
}

.list-abs {
  color: #777;
  font-size: 12px;
  border-bottom: 1px dashed #eee;
  padding: 20rpx 0 25rpx;
  max-height: 50px;
  overflow: hidden;
}

.list-money {
  color: #f66;
  padding-top: 15rpx;
  flex-direction: row;
}

.my-row {
  width: 100%;
  display: flex;
}

.my-col-4 {
  flex: 1;
  padding: 0 10rpx;
}

.gg-img {
  max-width: 100%;
  border-radius: 6px;
}

.gg-title {
  font-size: 13px;
  white-space: nowrap;
}

.gg-box {
  padding: 15px 20rpx 0;
}

/*通用页面*/

.goods-img {
  width: 100%;
}

.goods-footer {
  position: fixed;
  left: 0px;
  bottom: 0px;
  width: 100%;
  height: 50px;
  background-color: #fff;
  border-top: 1px solid #eee;
  z-index: 9999;
  line-height: 50px;
}

.footer-item {
  display: flex;
}

.footer-left {
  padding-left: 15px;
  width: 70%;
  color: #f66;
}

.footer-left text {
  font-size: 14px;
  color: #878787;
}

.footer-right {
  background: #9c9;
  color: #fff;
  width: 30%;
  text-align: center;
  font-size: 15px;
}

.goods-header {
  background: #fff;
  border-bottom: 1px solid #eee;
  margin-top: -10rpx;
}

.goods-title {
  color: #f66;
  margin-bottom: 15rpx;
  border-bottom: 1px dashed #eee;
  padding-bottom: 15rpx;
}

.goods-abs {
  padding-top: 5rpx;
  font-size: 12px;
  color: #878787;
}

.goods-content {
  border-top: 1px solid #eee;
  border-bottom: 1px solid #eee;
  margin: 5px 0;
  background: #fff;
  color: #777;
  font-size: 12px;
}

.goods-content-title {
  border-bottom: 1px dashed #eee;
  padding-bottom: 15rpx;
  color: #333;
  font-size: 14px;
}

.goods-content-info {
  padding-top: 15rpx;
}

.tui-title {
  font-size: 12px;
  padding-left: 15rpx;
  border-left: 6rpx solid #9c9;
}

.tui-title navigator {
  float: right;
  color: #878787;
}

.shouquan {
  line-height: 1.55555556;
  color: #9c9;
  background: #fff;
  font-size: 14px;
}

/*列表*/

.nav {
  width: 100%;
  white-space: nowrap;
  /* padding: 10rpx 0; */
  background: #fff;
  /* border-bottom: 1rpx solid #eee; */
}

.nav view {
  display: inline-block;
  padding: 15rpx 30rpx;
  font-size: 14px;
  color: #666;
}

.nav .current {
  color: #9c9;
  border-bottom: 2px solid #9c9;
}

.catetitle {
  text-align: center;
  margin-top: 20px;
  letter-spacing: 0.6em;
  padding-left: 0.6em;
  font-size: 24px;
  color: #9c9;
}

.gwc {
  /* position: fixed; */
  bottom: 10px;
  right: 15px;
}

.gwc image {
  width: 40px;
  height: 40px;
}

/* 通用弹窗 */

.shade {
  z-index: 999;
  background-color: rgba(0, 0, 0, 0.5);
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  position: fixed;
}

.shade .box {
  width: 450rpx;
  height: 300rpx;
  position: relative;
  margin: 0px auto;
  z-index: 9999;
  background-color: #fff;
  border-radius: 4px;
  top: 20%;
}

.shade .box .layui-layer-title {
  text-align:right;
   padding:5rpx 15rpx;
   color:#999;
}
.shade-login{
  text-align:center;
padding:20rpx;
color:#777;
font-size:14px;
}
.shade-btn{
  padding: 20rpx;
  
}
.shade-btn button{
  font-size: 14px;
}